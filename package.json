{
  "name": "nexcss-framework",
  "version": "1.2.1",
  "description": "A modern utility-first CSS framework with advanced features and framework integration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "nexcss": "scripts/cli.js"
  },
  "files": [
    "dist",
    "src",
    "scripts",
    "LICENSE",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./postcss": "./dist/postcss.js",
    "./css": "./dist/index.css"
  },
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "node scripts/build.js",
    "dev": "node scripts/build.js --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
    "prepare": "npm run build"
  },
  "peerDependencies": {
    "postcss": "^8.4.0"
  },
  "dependencies": {
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "commander": "^8.3.0"
  },
  "devDependencies": {
    "esbuild": "^0.14.0",
    "jest": "^27.4.0",
    "rimraf": "^3.0.2"
  },
  "keywords": [
    "css",
    "framework",
    "utility",
    "tailwind",
    "postcss",
    "responsive",
    "dark-mode"
  ],
  "author": "Your Name",
  "license": "MIT"
}
