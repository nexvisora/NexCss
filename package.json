{
  "name": "nexcss-framework",
  "version": "1.0.0",
  "description": "A modern utility-first CSS framework with advanced features and framework integration",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "type": "module",
  "bin": {
    "nexcss": "dist/cli.js"
  },
  "files": [
    "dist",
    "src",
    "templates",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "node scripts/build.js",
    "dev": "node scripts/build.js --watch",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "lint": "eslint . --max-warnings 50",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint && npm test",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "css",
    "framework",
    "utility",
    "tailwind",
    "postcss",
    "responsive",
    "dark-mode",
    "react",
    "vue",
    "svelte"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nexvisora/NexCss.git"
  },
  "bugs": {
    "url": "https://github.com/nexvisora/NexCss/issues"
  },
  "homepage": "https://nexcss.dev",
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.8.3",
    "chalk": "^5.3.0",
    "chokidar": "^3.5.3",
    "chrome-launcher": "^1.1.0",
    "color-namer": "^1.4.0",
    "commander": "^11.1.0",
    "css": "^3.0.0",
    "cssnano": "^6.0.2",
    "inquirer": "^9.2.12",
    "jsdom": "^23.0.1",
    "lighthouse": "^11.4.0",
    "ora": "^7.0.1",
    "postcss": "^8.4.32",
    "postcss-combine-duplicated-selectors": "^10.0.3",
    "postcss-combine-media-query": "^1.0.1",
    "postcss-custom-media": "^10.0.2",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.3.0",
    "postcss-sort-media-queries": "^5.2.0",
    "postcss-url": "^10.1.3",
    "puppeteer": "^21.6.1",
    "wcag-color": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.7",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "esbuild": "^0.19.10",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
